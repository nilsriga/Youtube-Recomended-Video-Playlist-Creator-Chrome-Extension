{
    "manifest_version": 2,
    "name": "Personal TV",
    "version": "1.0.0",
  
    "description": "This extension scrapes your recomened video id's from youtube's home page and put's them in a playlist called TV. ATTENTION. This Extension wors only if you are signed into chrome browser, and have turned on the 'sync' option. And you also have to be signed into youtube with the same account.",
    "icons": {"128": "icon_128.png"},
  
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Create a new playlist."
    },

    "background": {
        "scripts": ["dist/jquery.js", "dist/background.js"],
        "persistent": false
    },

    "author": "Nils Riga",

    "offline_enabled": false,

    "permissions": [
        "activeTab", 
        "identity",
        "tabs",
        "https://*/*"
    ],

    "key": "",

    "oauth2": {
        "client_id": "922133810139-4qe3vorg8gltpch761fs4m7vmp464rl4.apps.googleusercontent.com",
        "scopes":[
            "email",
            "profile",
            "https://www.googleapis.com/auth/youtube.force-ssl"
        ]
      }
  }